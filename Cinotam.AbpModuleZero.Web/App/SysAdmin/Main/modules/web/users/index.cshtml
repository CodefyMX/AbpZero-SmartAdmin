<div ng-controller="app.views.users.index as vm">
    <h1>@L("Users")</h1>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">@L("Users")</div>
                <div class="panel-body">
                    <user-selector onselected="vm.onSelected" actions="vm.actions"  properties="vm.properties"></user-selector>
                </div>
            </div>
        </div>
    </div>
</div>